{
	"format_version": "1.10.0",
	"minecraft:attachable": {
		"description": {
			"identifier": "starry_map:dynamic_anchor_point",
			"materials": {
				"enchanted": "armor_enchanted",
				"default": "armor"
			},
			"textures": {
				"enchanted": "textures/misc/enchanted_item_glint",
				"default": "textures/实体贴图/锚点虚印"
			},
			"geometry": {
				"default": "geometry.item.dynamic_anchor_point"
			},
			"scripts": {
				"animate": [
					"control_hold",
					"item_float"
				]
			},
			"animations": {
				// 持有动画切换控制器
				"control_hold": "controller.animation.item.world_of_box",
				// 浮动动画
				"item_float": "animation.item.dynamic_anchor_point.float",
				// 关闭动画
				"item_done": "animation.item.dynamic_anchor_point.close",
				// 打开动画
				"item_open": "animation.item.dynamic_anchor_point.open"
			},
			"render_controllers": [
				"controller.render.armor"
			]
		}
	}
}