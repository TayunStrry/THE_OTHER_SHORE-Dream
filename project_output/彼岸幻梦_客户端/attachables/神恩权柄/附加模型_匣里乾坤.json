{
	"format_version": "1.10.0",
	"minecraft:attachable": {
		"description": {
			"identifier": "starry_map:world_of_box",
			"materials": {
				"enchanted": "armor_enchanted",
				"default": "armor",
				"light": "opal_light_only"
			},
			"textures": {
				"enchanted": "textures/misc/enchanted_item_glint",
				"default": "textures/实体贴图/匣里乾坤"
			},
			"geometry": {
				"default": "geometry.item.world_of_box"
			},
			"scripts": {
				"animate": [
					"control_hold",
					"item_float"
				]
			},
			"animations": {
				// 持有动画切换控制器
				"control_hold": "controller.animation.item.world_of_box",
				// 浮动动画
				"item_float": "animation.item.world_of_box.float",
				// 关闭动画
				"item_done": "animation.item.world_of_box.close",
				// 打开动画
				"item_open": "animation.item.world_of_box.open"
			},
			"render_controllers": [
				"controller.render.world_of_box"
			]
		}
	}
}