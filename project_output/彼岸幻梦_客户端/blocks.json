{
    // 指定文件格式版本
    "format_version": [
        1,
        21,
        20
    ],
    // 地块类型: 草地 (grass)
    "grass": {
        "isotropic": {
            "up": true, // 上方纹理各向同性
            "down": true // 下方纹理各向同性
        },
        "textures": {
            "up": "grass_top", // 上方纹理
            "down": "grass_side", // 下方纹理
            "side": "grass_side" // 侧面纹理
        },
        "carried_textures": "grass_carried_top", // 玩家携带时使用的纹理
        "sound": "grass" // 与该地块相关联的声音
    },
    // 地块类型: 草路 (grass_path)
    "grass_path": {
        "isotropic": {
            "up": true,
            "down": true
        },
        "textures": "grass_path_top", // 上方纹理
        "sound": "grass" // 与该地块相关联的声音
    },
    // 地块类型: 腐殖土 (podzol)
    "podzol": {
        "isotropic": {
            "up": true,
            "down": true
        },
        "textures": "dirt_podzol_top", // 上方纹理
        "sound": "gravel" // 与该地块相关联的声音
    },
    // 地块类型: 菌丝土 (mycelium)
    "mycelium": {
        "isotropic": {
            "up": true,
            "down": true
        },
        "textures": "mycelium_top", // 上方纹理
        "sound": "grass" // 与该地块相关联的声音
    },
    // 地块类型: 绯红菌丝土 (crimson_nylium)
    "crimson_nylium": {
        "isotropic": {
            "up": true,
            "down": true
        },
        "textures": "crimson_nylium_top", // 上方纹理
        "sound": "grass" // 与该地块相关联的声音
    },
    // 地块类型: 幽暗菌丝土 (warped_nylium)
    "warped_nylium": {
        "isotropic": {
            "up": true,
            "down": true
        },
        //"carried_textures": "grass_path_top", // 玩家携带时使用的纹理"
        "textures": "warped_nylium_top", // 上方纹理
        "sound": "grass" // 与该地块相关联的声音
    },
    "starry_map:smooth_steps": {
        "sound": "snow"
    }
}