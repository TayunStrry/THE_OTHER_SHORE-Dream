{
	"format_version": "1.10.0",
	"render_controllers": {
		"controller.render.custom_allow_dyed": {
			"geometry": "Geometry.default",
			"materials": [
				{
					"*": "variable.is_dyed ? Material.dyed : Material.default"
				}
			],
			"textures": [
				"variable.is_dyed ? Texture.dyed : Texture.default",
				"Texture.default"
			]
		},
		"controller.render.custom_light": {
			"materials": [
				{
					"*": "material.light"
				}
			],
			"textures": [
				"texture.light"
			],
			"geometry": "geometry.light"
		},
		"controller.render.magic_equipment": {
			"materials": [
				{
					"*": "material.default"
				}
			],
			"textures": [
				"texture.default"
			],
			"geometry": "geometry.default"
		},
		"controller.render.display_heart": {
			"materials": [
				{
					"*": "material.light"
				}
			],
			"textures": [
				"texture.heart"
			],
			"overlay_color": {
				"r": "math.lerp(1, 0.00, query.health / query.max_health)",
				"g": "math.lerp(0, 0.65, query.health / query.max_health)",
				"b": "math.lerp(0, 0.85, query.health / query.max_health)",
				"a": 0.65
			},
			"geometry": "geometry.heart"
		},
		"controller.render.variant_aims": {
			"arrays": {
				"textures": {
					"array.texture": [
						"texture.void",
						"texture.aims"
					]
				}
			},
			"materials": [
				{
					"*": "material.default"
				}
			],
			"textures": [
				"array.texture[q.variant == 1 && q.has_target]"
			],
			"geometry": "geometry.aims"
		},
		"controller.render.inventory": {
			"part_visibility": [
				{
					"inventory": "q.is_chested"
				}
			],
			"materials": [
				{
					"*": "material.default"
				}
			],
			"textures": [
				"texture.inventory"
			],
			"geometry": "geometry.inventory"
		},
		"controller.render.blessing": {
			"arrays": {
				"textures": {
					"array.texture": [
						"texture.void",
						"texture.blessing"
					]
				}
			},
			"materials": [
				{
					"*": "v.enable_blessing ? material.blend_light : material.default"
				}
			],
			"uv_anim": {
				"offset": [
					"math.sin(q.life_time * 64) * 0.2",
					"math.cos(q.life_time * 64) * 0.2"
				],
				"scale": [
					1.0,
					1.0
				]
			},
			"textures": [
				"array.texture[v.enable_blessing]"
			],
			"light_color_multiplier": 1.5,
			"ignore_lighting": true,
			"geometry": "geometry.blessing"
		},
		"controller.render.chair": {
			"part_visibility": [
				{
					"chair": "!v.swim_amount && !q.is_riding"
				},
				{
					"chair_wool": "!v.swim_amount && !q.is_riding"
				},
				{
					"chair_frame": "!v.swim_amount && !q.is_riding"
				},
				{
					"chair_hang": "!v.swim_amount && !q.is_riding"
				}
			],
			"materials": [
				{
					"*": "material.default"
				},
				{
					"chair_hang": "material.blend"
				}
			],
			"textures": [
				"texture.chair"
			],
			"geometry": "geometry.chair"
		},
		"controller.render.aims": {
			"arrays": {
				"textures": {
					"array.texture": [
						"texture.void",
						"texture.aims"
					]
				}
			},
			"materials": [
				{
					"*": "material.default"
				}
			],
			"textures": [
				"array.texture[q.has_target]"
			],
			"geometry": "geometry.aims"
		}
	}
}