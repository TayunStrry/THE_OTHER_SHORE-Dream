            "arrays": {
                "textures": {
                    "array.skins": [
                        "texture.default",
                        "texture.angry"
                    ]
                }
            },
            "geometry": "Geometry.default",
            "materials": [
                {
                    "*": "Material.default"
                }
            ],
            "textures": [
                "array.skins[query.property('minecraft:has_angry')]"
