                {
                    "*": "Material.default"
                }
            ],
            "overlay_color": {
                "r": 1,
                "g": 0,
                "b": 0,
                "a": "math.lerp(1, 0, query.health / query.max_health)"
            },
            "textures": [
                "texture.elytrum"
            ]
        }
