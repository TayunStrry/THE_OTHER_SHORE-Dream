                    "pedals_right": "query.is_saddled"
                },
                {
                    "pedals_left": "query.is_saddled"
                },
                {
                    "saddled": "query.is_saddled"
                }
            ],
            "geometry": "geometry.default",
            "materials": [
                {
                    "*": "material.default"
                },
                {
                    "face": "material.light"
                }
            ],
            "textures": [
                "texture.facade"
            ]
        },
        "controller.render.enable_shelter": {
            "geometry": "geometry.default",
            "materials": [
                {
                    "*": "material.default"
                },
                {
                    "display_group": "material.opal_light_blend"
                },
                {
                    "display_group_inside": "material.opal_light_blend"
                }
            ],
            "textures": [
                "texture.default"
            ]
