        "controller.render.free_pointer": {
            "materials": [
                {
                    "*": "material.default"
                }
            ],
            "textures": [
                "texture.default"
            ],
            "uv_anim": {
                "offset": [
                    "math.cos(q.life_time * 64) * 0.2",
                    "math.sin(q.life_time * 64) * 0.2"
                ],
                "scale": [
                    1.0,
                    1.0
                ]
            },
            "overlay_color": {
                "r": "q.property('property:color_r')",
                "g": "q.property('property:color_g')",
                "b": "q.property('property:color_b')",
                "a": "q.property('property:color_a')"
            },
            "geometry": "geometry.default"
        },
        "controller.render.dawn_glow": {
            "geometry": "geometry.default",
            "materials": [
                {
                    "*": "material.default"
                },
                {
                    "star0": "material.light"
                },
                {
                    "star1": "material.light"
                },
                {
                    "star2": "material.light"
                },
                {
                    "power0": "material.light"
                },
                {
                    "power1": "material.light"
