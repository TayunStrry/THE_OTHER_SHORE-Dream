{
	"format_version": "1.8.0",
	"render_controllers": {
		"controller.render.experience_orb": {
			"geometry": "geometry.default",
			"materials": [
				{
					"*": "material.default"
				}
			],
			"textures": [
				"texture.default"
			],
			"overlay_color": {
				"r": 0.75,
				"g": 0.35,
				"b": 0.60,
				"a": 0.65
			},
			"uv_anim": {
				"offset": [
					"variable.u",
					"variable.v"
				],
				"scale": [
					1,
					1
				]
			}
		},
		"controller.render.wither_boss": {
			"arrays": {
				"textures": {
					"array.wither_state": [
						"texture.invulnerable",
						"texture.default"
					]
				}
			},
			"geometry": "geometry.default",
			"materials": [
				{
					"*": "material.default"
				}
			],
			"textures": [
				"array.wither_state[variable.display_normal_skin]"
			],
			"overlay_color": {
				"r": "variable.is_invulnerable ? 0.0 : this",
				"g": "variable.is_invulnerable ? 0.0 : this",
				"b": "variable.is_invulnerable ? 0.5 : this",
				"a": "variable.is_invulnerable ? math.max(0.75 - q.life_time * 0.1, 0) : this"
			}
		}
	}
}