{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.item.magic_crystal_bow.sneaking": {
			"initial_state": "stretch",
			"states": {
				"stretch": {
					"animations": [
						"stretch"
					],
					"transitions": [
						{
							"sneaking": "q.is_sneaking == 1 && q.all_animations_finished"
						}
					]
				},
				"sneaking": {
					"animations": [
						"sneaking"
					],
					"transitions": [
						{
							"stretch": "q.is_sneaking == 0 && q.all_animations_finished"
						}
					]
				}
			}
		},
		"controller.animation.item.magic_crystal_bow.power": {
			"initial_state": "default",
			"states": {
				"power": {
					"animations": [
						"power"
					],
					"transitions": [
						{
							"release": "!q.is_using_item && q.all_animations_finished"
						}
					]
				},
				"release": {
					"animations": [
						"release",
						"homing"
					],
					"transitions": [
						{
							"power": "q.is_using_item && q.all_animations_finished"
						}
					]
				},
				"default": {
					"animations": [
						"homing"
					],
					"transitions": [
						{
							"power": "q.is_using_item && q.all_animations_finished"
						}
					]
				}
			}
		},
		"controller.animation.item.magic_literature_book": {
			"initial_state": "open",
			"states": {
				"open": {
					"animations": [
						"item_open",
						"item_flip"
					],
					"transitions": [
						{
							"close": "q.is_on_ground == 0 && q.is_in_contact_with_water == 1 && q.all_animations_finished"
						},
						{
							"close": "q.is_sprinting == 1 && q.is_on_ground == 0 && q.all_animations_finished"
						},
						{
							"close": "q.is_in_contact_with_water == 1 && q.all_animations_finished"
						}
					]
				},
				"close": {
					"animations": [
						"item_done"
					],
					"transitions": [
						{
							"open": "q.is_on_ground == 1 && q.is_sprinting == 0 && q.is_in_contact_with_water == 0 && q.all_animations_finished"
						}
					]
				}
			}
		},
		"controller.animation.item.is_sneaking.first_person": {
			"initial_state": "open",
			"states": {
				"open": {
					"animations": [
						"first_person_open"
					],
					"transitions": [
						{
							"close": "q.is_sneaking == 1 && q.all_animations_finished"
						}
					]
				},
				"close": {
					"animations": [
						"first_person_done"
					],
					"transitions": [
						{
							"open": "q.is_sneaking == 0 && q.all_animations_finished"
						}
					],
					"blend_transition": 0.25,
					"blend_via_shortest_path": true
				}
			}
		},
		"controller.animation.item.is_sneaking.default": {
			"initial_state": "open",
			"states": {
				"open": {
					"animations": [
						"default_open"
					],
					"transitions": [
						{
							"close": "q.is_sneaking == 1 && q.all_animations_finished"
						}
					]
				},
				"close": {
					"animations": [
						"default_done"
					],
					"transitions": [
						{
							"open": "q.is_sneaking == 0 && q.all_animations_finished"
						}
					],
					"blend_transition": 0.25,
					"blend_via_shortest_path": true
				}
			}
		},
		"controller.animation.item.world_of_box": {
			"initial_state": "close",
			"states": {
				"open": {
					"animations": [
						"item_open"
					],
					"transitions": [
						{
							"close": "q.is_on_ground == 0 && q.is_in_contact_with_water == 1 && q.all_animations_finished"
						},
						{
							"close": "q.is_sprinting == 1 && q.is_on_ground == 0 && q.all_animations_finished"
						},
						{
							"close": "q.is_in_contact_with_water == 1 && q.all_animations_finished"
						}
					]
				},
				"close": {
					"animations": [
						"item_done"
					],
					"transitions": [
						{
							"open": "q.is_on_ground == 1 && q.is_sprinting == 0 && q.is_in_contact_with_water == 0 && q.all_animations_finished"
						}
					]
				}
			}
		}
	}
}