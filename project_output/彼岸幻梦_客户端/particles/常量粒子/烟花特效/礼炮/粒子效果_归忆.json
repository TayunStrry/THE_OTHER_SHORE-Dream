{
    "format_version": "1.10.0",
    "particle_effect": {
        "description": {
            "identifier": "constant:fireworks_salvo_rune_orange",
            "basic_render_parameters": {
                "material": "particles_alpha",
                "texture": "textures/particle/particles"
            }
        },
        "components": {
            "minecraft:particle_appearance_tinting": {
                "color": [
                    1.0,
                    0.5,
                    0.0,
                    "variable.particle_age > (variable.particle_lifetime / Math.random(2.0, 1.0)) ? 0.45 : 1.00"
                ]
            },
            "minecraft:emitter_lifetime_events": {
                "timeline": {
                    "1.80": [
                        "event_particle:animation_phase_0"
                    ],
                    "1.85": [
                        "event_particle:animation_phase_1"
                    ]
                }
            },
            "minecraft:emitter_initialization": {
                "creation_expression": "variable.radius = 0.25; variable.radius = 1.0;"
            },
            "minecraft:emitter_local_space": {
                "position": true,
                "rotation": true
            },
            "minecraft:emitter_rate_steady": {
                "max_particles": 8,
                "spawn_rate": 100
            },
            "minecraft:emitter_lifetime_looping": {
                "active_time": 2
            },
            "minecraft:particle_initial_speed": [
                0,
                5,
                0
            ],
            "minecraft:emitter_shape_point": {
                "offset": [
                    0,
                    -10,
                    0
                ]
            },
            "minecraft:particle_lifetime_expression": {
                "max_lifetime": 2
            },
            "minecraft:particle_appearance_billboard": {
                "size": [
                    "variable.radius * math.max(1 - variable.particle_age, 0.3)",
                    "variable.radius * math.max(1 - variable.particle_age, 0.3)"
                ],
                "facing_camera_mode": "rotate_xyz",
                "uv": {
                    "texture_height": 128,
                    "texture_width": 128,
                    "uv": [
                        32,
                        88
                    ],
                    "uv_size": [
                        8,
                        8
                    ],
                    "stretch_to_lifetime": true,
                    "frames_per_second": 2,
                    "max_frame": 5,
                    "loop": true
                }
            },
            "minecraft:particle_motion_collision": {
                "collision_drag": 0.5,
                "coefficient_of_restitution": 1.0,
                "collision_radius": 0.1
            },
            "minecraft:particle_motion_dynamic": {}
        },
        "events": {
            "event_particle:animation_phase_0": {
                "sequence": [
                    {
                        "particle_effect": {
                            "effect": "constant:erupt_rune_orange",
                            "type": "emitter"
                        }
                    },
                    {
                        "particle_effect": {
                            "effect": "constant:excite_rune_orange",
                            "type": "emitter"
                        }
                    }
                ]
            },
            "event_particle:animation_phase_1": {
                "sequence": [
                    {
                        "particle_effect": {
                            "effect": "constant:fireworks_paper_rune_orange",
                            "type": "emitter"
                        }
                    },
                    {
                        "particle_effect": {
                            "effect": "constant:fireworks_fireball_rune_orange",
                            "type": "emitter_bound"
                        }
                    },
                    {
                        "particle_effect": {
                            "effect": "constant:fireworks_sparkle",
                            "type": "emitter_bound"
                        }
                    }
                ]
            }
        }
    }
}