{
	// 定义配置文件的格式版本
	"format_version": "1.13.0",
	// 定义一个Minecraft中的树特征
	"minecraft:tree_feature": {
		// 特征的描述信息
		"description": {
			// 特征的唯一标识符
			"identifier": "feature:tree_giant_oak_tree"
		},
		// 定义树干的复杂配置
		"fancy_trunk": {
			// 树干高度的配置
			"trunk_height": {
				// 基本高度
				"base": 24,
				// 高度的随机变化范围
				"variance": 16,
				// 高度的缩放因子
				"scale": 0.618
			},
			// 树枝的配置
			"branches": {
				// 树枝的倾斜度
				"slope": 0.618,
				// 树枝的密度
				"density": 0.45,
				// 树枝生成的最小海拔因子
				"min_altitude_factor": 0.2
			},
			// 树干宽度的缩放因子
			"width_scale": 1,
			// 树干的基本宽度
			"trunk_width": 2,
			// 树干使用的方块材质
			"trunk_block": "minecraft:oak_log",
			// 叶子生成的海拔因子
			"foliage_altitude_factor": 0.3
		},
		// 定义树冠的复杂配置
		"fancy_canopy": {
			// 树冠的高度
			"height": 3,
			// 树冠的半径
			"radius": 5,
			// 树叶使用的方块材质
			"leaf_block": "minecraft:oak_leaves"
		},
		// 树可以生成的基础方块列表
		"base_block": [
			"minecraft:dirt",
			"minecraft:podzol",
			"minecraft:grass_block",
			"minecraft:coarse_dirt"
		],
		// 树可以生长在其上的方块列表
		"may_grow_on": [
			"minecraft:dirt",
			"minecraft:podzol",
			"minecraft:grass_block",
			"minecraft:coarse_dirt"
		],
		// 树在生成时可以替换的方块列表
		"may_replace": [
			"minecraft:air",
			"minecraft:oak_leaves",
			"minecraft:birch_leaves",
			"minecraft:spruce_leaves",
			"minecraft:jungle_leaves",
			"minecraft:acacia_leaves",
			"minecraft:cherry_leaves",
			"minecraft:azalea_leaves",
			"minecraft:dark_oak_leaves",
			"minecraft:mangrove_leaves"
		],
		// 树在生成时可以穿透的方块列表
		"may_grow_through": [
			"minecraft:dirt",
			"minecraft:podzol",
			"minecraft:grass_block",
			"minecraft:coarse_dirt"
		]
	}
}