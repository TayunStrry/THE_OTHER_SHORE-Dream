{
	"format_version": "1.21.10",
	"minecraft:entity": {
		"description": {
			"identifier": "starry_map:execute.virtual_target",
			"is_experimental": false,
			"is_summonable": true,
			"is_spawnable": false
		},
		"component_groups": {
			// 实体销毁
			"entity_module:despawn": {
				"minecraft:instant_despawn": {
					"remove_child_entities": true
				}
			}
		},
		"components": {
			// 基础属性
			"minecraft:collision_box": {
				"height": 1,
				"width": 0.5
			},
			"minecraft:type_family": {
				"family": [
					"endermite",
					"skeleton",
					"chicken",
					"monster",
					"player",
					"fox"
				]
			},
			"minecraft:health": {
				"value": 99999999,
				"min": 1
			},
			"minecraft:scale": {
				"value": 2
			},
			"minecraft:knockback_resistance": {
				"value": 1
			},
			"minecraft:nameable": {
				"always_show": true
			},
			"minecraft:fire_immune": true,
			"minecraft:is_collidable": {},
			// 物理属性
			"minecraft:pushable": {
				"is_pushable_by_piston": false,
				"is_pushable": false
			},
			"minecraft:physics": {
				"push_towards_closest_space": true,
				"has_collision": true,
				"has_gravity": true
			},
			"minecraft:body_rotation_axis_aligned": {},
			//事件触发器
			"minecraft:interact": {
				"interactions": [
					{
						"cooldown": 1,
						"use_item": false,
						"interact_text": "移除 < 虚物靶标 >",
						"play_sounds": "armor.equip_leather",
						"on_interact": {
							"filters": {
								"test": "is_sneaking",
								"subject": "player",
								"operator": "=="
							},
							"event": "entity_event:delete",
							"target": "self"
						}
					}
				]
			},
			// 行为控制
			"minecraft:conditional_bandwidth_optimization": {
				"conditional_values": {
					"max_dropped_ticks": 20,
					"max_optimized_distance": 200,
					"use_motion_prediction_hints": true
				},
				"default_values": {
					"max_dropped_ticks": 10,
					"max_optimized_distance": 100,
					"use_motion_prediction_hints": true
				}
			},
			"minecraft:navigation.hover": {
				"can_path_from_air": true,
				"is_amphibious": true,
				"can_swim": true,
				"can_sink": true
			},
			"minecraft:movement.hover": {}
		},
		"events": {
			// 实体销毁事件
			"entity_event:delete": {
				"add": {
					"component_groups": [
						"entity_module:despawn"
					]
				}
			}
		}
	}
}