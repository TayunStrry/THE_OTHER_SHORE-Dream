{
	"format_version": "1.21.10",
	"minecraft:entity": {
		"description": {
			"identifier": "starry_map:elves.jellyfish_of_pearl",
			"is_spawnable": true,
			"is_summonable": true,
			"is_experimental": false
		},
		"component_groups": {
			// 发动攻击后
			"entity_module:angry_after": {
				// 事件触发器
				"minecraft:angry": {
					"duration": 32,
					"broadcast_anger": true,
					"broadcast_range": 24,
					"broadcast_targets": [
						"starry"
					],
					"broadcast_filters": {
						"test": "is_visible",
						"subject": "other",
						"value": true
					},
					"filters": {
						"test": "is_visible",
						"subject": "target",
						"value": true
					},
					"calm_event": {
						"event": "entity_event:attack_done",
						"target": "self"
					}
				},
				"minecraft:timer": {
					"time": 2,
					"time_down_event": {
						"event": "entity_event:attack_execute",
						"target": "self"
					}
				}
			},
			// 淡化退场
			"entity_module:downplay": {
				"minecraft:scale_by_age": {
					"start_scale": 1.0,
					"end_scale": 0
				},
				"minecraft:ageable": {
					"duration": 1,
					"grow_up": {
						"event": "entity_event:delete",
						"target": "self"
					}
				}
			},
			// 实体销毁
			"entity_module:despawn": {
				"minecraft:scale": {
					"value": 0
				},
				"minecraft:instant_despawn": {
					"remove_child_entities": true
				}
			}
		},
		"components": {
			"minecraft:type_family": {
				"family": [
					"in_pearl",
					"spirit"
				]
			},
			"minecraft:health": {
				"value": 10
			},
			"minecraft:scale": {
				"value": 1.0
			},
			"minecraft:flying_speed": {
				"value": 0.15
			},
			"minecraft:underwater_movement": {
				"value": 0.15
			},
			"minecraft:collision_box": {
				"width": 0.1,
				"height": 0.1
			},
			"minecraft:damage_sensor": {
				"triggers": [
					{
						"cause": "all",
						"deals_damage": false
					}
				]
			},
			"minecraft:ageable": {
				"duration": 30,
				"grow_up": {
					"event": "entity_event:downplay",
					"target": "self"
				}
			},
			"minecraft:breathable": {
				"totalSupply": 10,
				"suffocateTime": 0,
				"breathes_air": true,
				"breathes_lava": true,
				"breathes_water": true,
				"breathes_solids": true
			},
			"minecraft:body_rotation_blocked": {},
			"minecraft:body_rotation_axis_aligned": {},
			// 事件触发器
			"minecraft:on_target_acquired": {
				"event": "entity_event:attack_point",
				"target": "self"
			},
			"minecraft:despawn": {
				"despawn_from_distance": {}
			},
			// 行为控制
			"minecraft:navigation.hover": {
				"can_path_from_air": true,
				"is_amphibious": true,
				"can_swim": true,
				"can_sink": true
			},
			"minecraft:behavior.nearest_attackable_target": {
				"priority": 9,
				"entity_types": [
					{
						"filters": {
							"any_of": [
								{
									"test": "is_family",
									"subject": "other",
									"value": "abyss"
								},
								{
									"test": "is_family",
									"subject": "other",
									"value": "monster"
								},
								{
									"test": "is_family",
									"subject": "other",
									"value": "warden"
								},
								{
									"test": "is_family",
									"subject": "other",
									"value": "hoglin"
								}
							]
						},
						"within_default": 32
					}
				],
				"must_reach": true,
				"must_see": true
			},
			"minecraft:behavior.follow_mob": {
				"priority": 0,
				"filters": {
					"test": "is_family",
					"subject": "other",
					"value": "divine_favor_guide.pearl"
				},
				"speed_multiplier": 1,
				"stop_distance": 2,
				"search_range": 32
			},
			"minecraft:behavior.random_hover": {
				"priority": 5,
				"xz_dist": 8,
				"y_dist": 4,
				"y_offset": -1,
				"interval": 1,
				"hover_height": [
					0,
					1
				]
			},
			"minecraft:behavior.float": {
				"priority": 10
			},
			"minecraft:pushable": {
				"is_pushable_by_piston": true,
				"is_pushable": true
			},
			"minecraft:physics": {
				"push_towards_closest_space": true,
				"has_collision": true,
				"has_gravity": true
			},
			"minecraft:movement.hover": {},
			"minecraft:fire_immune": true,
			"minecraft:is_stackable": {},
			"minecraft:can_fly": {}
		},
		"events": {
			// 实体攻击瞄准事件
			"entity_event:attack_point": {
				"add": {
					"component_groups": [
						"entity_module:angry_after"
					]
				}
			},
			// 实体攻击执行事件
			"entity_event:attack_execute": {
				"queue_command": {
					"command": [
						"playsound conduit.short @a[r=8] ~~~",
						"particle constant:impact_rune_blue ~~~",
						"opal:divine_favor_attack @s jellyfish_of_pearl"
					]
				}
			},
			// 实体攻击结束事件
			"entity_event:attack_done": {
				"remove": {
					"component_groups": [
						"entity_module:angry_after"
					]
				}
			},
			// 实体淡化退场事件
			"entity_event:downplay": {
				"add": {
					"component_groups": [
						"entity_module:downplay"
					]
				}
			},
			// 实体销毁事件
			"entity_event:delete": {
				"add": {
					"component_groups": [
						"entity_module:despawn"
					]
				}
			}
		}
	}
}