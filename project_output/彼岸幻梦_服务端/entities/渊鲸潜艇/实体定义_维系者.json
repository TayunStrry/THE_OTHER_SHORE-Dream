{
	"format_version": "1.21.10",
	"minecraft:entity": {
		"description": {
			"identifier": "starry_map:abyss_whale.support",
			"is_experimental": false,
			"is_summonable": true,
			"is_spawnable": true
		},
		"component_groups": {
			// 签订契约之前
			"entity_module:contract_before": {
				"minecraft:experience_reward": {
					"on_death": "query.last_hit_by_player ? 15 : 1"
				},
				"minecraft:behavior.random_swim": {
					"avoid_surface": true,
					"priority": 30
				},
				"minecraft:behavior.swim_wander": {
					"priority": 25
				},
				"minecraft:behavior.swim_idle": {
					"priority": 35
				},
				"minecraft:navigation.generic": {
					"can_path_over_water": true,
					"is_amphibious": true,
					"can_breach": false,
					"can_walk": false,
					"can_sink": false,
					"can_swim": true
				},
				"minecraft:behavior.tempt": {
					"speed_multiplier": 1,
					"priority": 1,
					"items": [
						"minecraft:enchanted_golden_apple",
						"starry_map:eternal_energy"
					]
				},
				"minecraft:despawn": {
					"despawn_from_distance": {}
				},
				"minecraft:movement.generic": {}
			},
			// 签订契约之后
			"entity_module:contract_after": {
				"minecraft:behavior.owner_hurt_by_target": {
					"priority": 7
				},
				"minecraft:behavior.owner_hurt_target": {
					"priority": 8
				},
				"minecraft:behavior.tempt": {
					"speed_multiplier": 1,
					"priority": 1,
					"items": [
						"starry_map:magic_crystal_screwdriver",
						"starry_map:magic_crystal_marbles",
						"starry_map:magic_crystal_shield",
						"starry_map:magic_crystal_wrench",
						"starry_map:magic_crystal_claw",
						"starry_map:magic_crystal_bow",
						"starry_map:magic_crystal_key"
					]
				},
				"minecraft:behavior.controlled_by_player": {
					"priority": 0
				},
				"minecraft:navigation.generic": {
					"can_path_over_water": true,
					"avoid_damage_blocks": true,
					"can_pass_doors": true,
					"can_open_doors": false,
					"is_amphibious": true,
					"can_swim": true,
					"can_walk": true
				},
				"minecraft:leashable": {
					"soft_distance": 5.0,
					"max_distance": 15.0,
					"hard_distance": 10.0,
					"on_leash": {
						"event": "minecraft:on_leash",
						"target": "self"
					},
					"on_unleash": {
						"event": "minecraft:on_unleash",
						"target": "self"
					}
				},
				"minecraft:scheduler": {
					"min_delay_secs": 4,
					"max_delay_secs": 8,
					"scheduled_events": [
						{
							"filters": {
								"test": "is_underwater",
								"operator": "==",
								"value": true
							},
							"event": "entity_event:oxygen_supply",
							"target": "self"
						},
						{
							"filters": {
								"all_of": [
									{
										"test": "is_underwater",
										"operator": "==",
										"value": false
									},
									{
										"test": "on_ground",
										"operator": "==",
										"value": false
									}
								]
							},
							"event": "entity_event:oxygen_cease",
							"target": "self"
						},
						{
							"filters": {
								"all_of": [
									{
										"test": "is_underwater",
										"operator": "==",
										"value": false
									},
									{
										"test": "on_ground",
										"operator": "==",
										"value": true
									}
								]
							},
							"event": "entity_event:submarine_stranded",
							"target": "self"
						}
					]
				},
				"minecraft:interact": {
					"interactions": [
						{
							"on_interact": {
								"filters": {
									"all_of": [
										{
											"test": "is_sneaking",
											"subject": "player",
											"operator": "=="
										},
										{
											"test": "is_owner",
											"subject": "other",
											"value": true
										}
									]
								},
								"event": "entity_event:recycling",
								"target": "self"
							},
							"interact_text": "回收 < 渊鲸炮击潜艇 >",
							"play_sounds": "armor.equip_diamond",
							"use_item": false,
							"cooldown": 1
						}
					]
				},
				"minecraft:rideable": {
					"seat_count": 2,
					"family_types": [
						"divine_favor_guide",
						"player"
					],
					"pull_in_entities": true,
					"seats": [
						{
							"position": [
								0.0,
								0.425,
								-0.15
							],
							"min_rider_count": 0,
							"max_rider_count": 2,
							"rotate_rider_by": 0
						},
						{
							"position": [
								0.0,
								0.425,
								0.25
							],
							"min_rider_count": 2,
							"max_rider_count": 2,
							"rotate_rider_by": 0
						}
					]
				},
				"minecraft:on_death": {
					"event": "entity_event:death",
					"target": "self"
				},
				"minecraft:input_ground_controlled": {},
				"minecraft:movement.basic": {},
				"minecraft:is_tamed": {}
			},
			// 初始状态
			"entity_module:initialize": {
				"minecraft:ageable": {
					"duration": 5,
					"grow_up": {
						"event": "entity_event:initialize_after",
						"target": "self"
					}
				}
			},
			// 巡逻状态
			"entity_module:patrol": {
				"minecraft:behavior.nearest_attackable_target": {
					"priority": 10,
					"entity_types": [
						{
							"filters": {
								"any_of": [
									{
										"test": "is_family",
										"subject": "other",
										"value": "abyss"
									},
									{
										"test": "is_family",
										"subject": "other",
										"value": "monster"
									},
									{
										"test": "is_family",
										"subject": "other",
										"value": "warden"
									},
									{
										"test": "is_family",
										"subject": "other",
										"value": "hoglin"
									}
								]
							},
							"within_default": 64
						}
					],
					"must_reach": false,
					"must_see": true
				},
				"minecraft:behavior.hurt_by_target": {
					"priority": 12
				},
				"minecraft:on_target_acquired": {
					"event": "entity_event:attack_point",
					"target": "self"
				}
			},
			// 发动攻击后
			"entity_module:angry_after": {
				"minecraft:angry": {
					"duration": 4,
					"broadcast_anger": true,
					"broadcast_range": 32,
					"broadcast_targets": [
						"starry",
						"whale"
					],
					"broadcast_filters": {
						"test": "is_visible",
						"subject": "other",
						"value": true
					},
					"filters": {
						"test": "is_visible",
						"subject": "target",
						"value": true
					},
					"calm_event": {
						"event": "entity_event:attack_done",
						"target": "self"
					}
				}
			},
			// 浮力模式
			"entity_module:buoyancy_model": {
				"minecraft:buoyant": {
					"base_buoyancy": 0.1,
					"apply_gravity": false,
					"simulate_waves": true,
					"liquid_blocks": [
						"minecraft:water",
						"minecraft:flowing_water"
					]
				}
			},
			// 氧气供给
			"entity_module:oxygen_supply": {
				"minecraft:timer": {
					"time": 5.0,
					"time_down_event": {
						"event": "entity_event:execute_oxygen_supply",
						"target": "self"
					}
				}
			},
			// 航行许可
			"entity_module:cruise_license": {
				"minecraft:movement": {
					"value": 0.05
				},
				"minecraft:underwater_movement": {
					"value": 0.35
				}
			},
			// 禁止航行
			"entity_module:cruise_prohibit": {
				"minecraft:movement": {
					"value": 0.0
				},
				"minecraft:underwater_movement": {
					"value": 0.0
				}
			}
		},
		"components": {
			// 基础属性
			"minecraft:type_family": {
				"family": [
					"submarine",
					"starry",
					"whale"
				]
			},
			"minecraft:collision_box": {
				"width": 0.5,
				"height": 1.0
			},
			"minecraft:health": {
				"value": 60
			},
			"minecraft:scale": {
				"value": 3
			},
			"minecraft:knockback_resistance": {
				"value": 1.0
			},
			"minecraft:nameable": {
				"allow_name_tag_renaming": true,
				"always_show": true
			},
			"minecraft:variable_max_auto_step": {
				"jump_prevented_value": 2.5,
				"controlled_value": 3,
				"base_value": 2.0
			},
			"minecraft:is_stackable": {},
			"minecraft:fire_immune": true,
			// 物理属性
			"minecraft:pushable": {
				"is_pushable_by_piston": true,
				"is_pushable": true
			},
			"minecraft:physics": {
				"push_towards_closest_space": true,
				"has_collision": true,
				"has_gravity": true
			},
			// 事件触发器
			"minecraft:damage_sensor": {
				"triggers": [
					{
						"cause": "all",
						"on_damage": {
							"event": "entity_event:under_attack",
							"target": "self",
							"filters": {
								"any_of": [
									{
										"test": "is_family",
										"subject": "damager",
										"value": "player"
									},
									{
										"test": "is_family",
										"subject": "damager",
										"value": "starry"
									}
								]
							}
						},
						"deals_damage": false
					},
					{
						"cause": "fall",
						"deals_damage": false
					},
					{
						"cause": "projectile",
						"deals_damage": true
					}
				]
			},
			"minecraft:tameable": {
				"probability": 1,
				"tame_items": [
					"minecraft:enchanted_golden_apple",
					"starry_map:eternal_energy"
				],
				"tame_event": {
					"event": "entity_event:create_contract_before",
					"target": "self"
				}
			},
			// 行为控制
			"minecraft:movement.generic": {
				"max_turn": 1
			}
		},
		"events": {
			// 实体诞生事件
			"minecraft:entity_spawned": {
				"add": {
					"component_groups": [
						"entity_module:contract_before",
						"entity_module:cruise_license",
						"entity_module:initialize"
					]
				}
			},
			// 实体死亡事件
			"entity_event:death": {
				"queue_command": {
					"command": [
						"opal:entity_death_preservation @s"
					]
				}
			},
			// 实体初始化后事件
			"entity_event:initialize_after": {
				"add": {
					"component_groups": [
						"entity_module:patrol"
					]
				},
				"remove": {
					"component_groups": [
						"entity_module:initialize"
					]
				}
			},
			// 实体攻击瞄准事件
			"entity_event:attack_point": {
				"sequence": [
					{
						"filters": {
							"all_of": [
								{
									"test": "has_component",
									"value": "minecraft:is_tamed",
									"subject": "target",
									"operator": "!="
								}
							]
						},
						"queue_command": {
							"command": [
								"execute as @s [tag=is_tamed] run title @a[r=32] actionbar 【 @s 】: 目标已锁定 正在引导舰炮攻击......",
								"opal:abyss_whale_attack @s execute"
							]
						},
						"add": {
							"component_groups": [
								"entity_module:angry_after"
							]
						}
					}
				]
			},
			// 实体攻击结束事件
			"entity_event:attack_done": {
				"queue_command": {
					"command": [
						"execute as @s [tag=is_tamed] run title @a[r=32] actionbar 【 @s 】: 目标已丢失 正在重新寻找目标......"
					]
				},
				"remove": {
					"component_groups": [
						"entity_module:angry_after"
					]
				}
			},
			// 实体契约事件
			"entity_event:create_contract_before": {
				"queue_command": {
					"command": [
						"give @p starry_map:mechanized_operation",
						"opal:entity_contract_rite",
						"tag @s add is_tamed"
					]
				},
				"add": {
					"component_groups": [
						"entity_module:contract_after",
						"entity_module:buoyancy_model"
					]
				},
				"remove": {
					"component_groups": [
						"entity_module:contract_before"
					]
				}
			},
			// 潜艇上浮事件
			"entity_event:immediately_float": {
				"queue_command": {
					"command": [
						"execute as @s [tag=is_tamed] run title @p[r=5] actionbar 【 @s 】: 正在< 上浮 >"
					]
				},
				"add": {
					"component_groups": [
						"entity_module:buoyancy_model"
					]
				}
			},
			// 潜艇下潜事件
			"entity_event:immediately_dive": {
				"queue_command": {
					"command": [
						"execute as @s [tag=is_tamed] run title @p[r=5] actionbar 【 @s 】: 正在< 下潜 >"
					]
				},
				"remove": {
					"component_groups": [
						"entity_module:buoyancy_model"
					]
				}
			},
			// 许可航行事件
			"entity_event:secure_prohibit": {
				"add": {
					"component_groups": [
						"entity_module:cruise_license"
					]
				},
				"remove": {
					"component_groups": [
						"entity_module:cruise_prohibit"
					]
				}
			},
			// 潜艇搁浅事件
			"entity_event:submarine_stranded": {
				"queue_command": {
					"command": [
						"execute as @s [tag=is_tamed] run title @p[r=5] actionbar 【 @s 】: 船只已经进入[§4 搁浅状态 §r], 请摆脱[§4 搁浅状态 §r] !"
					]
				},
				"add": {
					"component_groups": [
						"entity_module:cruise_prohibit"
					]
				},
				"remove": {
					"component_groups": [
						"entity_module:cruise_license"
					]
				}
			},
			// 氧气供应事件
			"entity_event:oxygen_supply": {
				"sequence": [
					{
						"queue_command": {
							"command": [
								"execute as @s [tag=is_tamed] run title @p[r=5] actionbar 【 @s 】: 正在开启< 氧气供应 >",
								"effect @p[r=5] conduit_power 16 0 true"
							]
						}
					},
					{
						"add": {
							"component_groups": [
								"entity_module:oxygen_supply"
							]
						}
					}
				]
			},
			// 氧气停供事件
			"entity_event:oxygen_cease": {
				"sequence": [
					{
						"queue_command": {
							"command": [
								"execute as @s [tag=is_tamed] run title @p[r=5] actionbar 【 @s 】: 正在关闭< 氧气供应 >",
								"effect @p[r=5] conduit_power 0 1 true"
							]
						}
					},
					{
						"remove": {
							"component_groups": [
								"entity_module:oxygen_supply"
							]
						}
					}
				]
			},
			// 执行氧气供应事件
			"entity_event:execute_oxygen_supply": {
				"queue_command": {
					"command": [
						"effect @s instant_health 1 0 true",
						"effect @p[r=5] conduit_power 16 0 true"
					]
				}
			},
			// 潜艇回收事件
			"entity_event:recycling": {
				"queue_command": {
					"command": [
						"opal:unload_package_inventory @s starry_map:call_whale_support",
						"particle constant:fireworks_fireball_rune_blue ~~~",
						"particle constant:fireworks_paper_rune_blue ~~~"
					]
				}
			}
		}
	}
}