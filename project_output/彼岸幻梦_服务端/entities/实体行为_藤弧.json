{
	"format_version": "1.16.0",
	"minecraft:entity": {
		"description": {
			"identifier": "starry_map:execute.rattan_arc",
			"is_spawnable": true,
			"is_summonable": true,
			"is_experimental": false,
			"scripts": {
				"animate": [
					"flying"
				]
			},
			"animations": {
				"flying": "controller.animation.server.apply_dynamic_flight_to_entity",
				"timer_0": "animation.server.second_timed_animation_1.5",
				"timer_1": "animation.server.second_timed_animation_2"
			}
		},
		"component_groups": {
			// 契约并驾驶实体
			"entity_module:controlled": {
				// 行为控制
				"minecraft:behavior.controlled_by_player": {
					"priority": 0
				},
				"minecraft:behavior.player_ride_tamed": {},
				// 基础参数
				"minecraft:horse.jump_strength": {
					"value": {
						"range_min": 2.0,
						"range_max": 4.0
					}
				},
				"minecraft:despawn": {
					"despawn_from_distance": {}
				},
				"minecraft:rideable": {
					"crouching_skip_interact": true,
					"seat_count": 1,
					"family_types": [
						"player"
					],
					"pull_in_entities": true,
					"seats": [
						{
							"position": [
								0.0,
								0.45,
								0.0
							],
							"min_rider_count": 0,
							"max_rider_count": 1,
							"rotate_rider_by": 0
						}
					]
				},
				"minecraft:buoyant": {
					"base_buoyancy": 0.1,
					"apply_gravity": false,
					"simulate_waves": true,
					"liquid_blocks": [
						"minecraft:water",
						"minecraft:flowing_water"
					]
				},
				"minecraft:ageable": {
					"duration": 5
				},
				"minecraft:scale_by_age": {
					"start_scale": 0.75,
					"end_scale": 1.75
				},
				"minecraft:input_ground_controlled": {},
				"minecraft:can_power_jump": {},
				"minecraft:is_saddled": {},
				"minecraft:is_tamed": {},
				// 事件触发器
				"minecraft:leashable": {
					"soft_distance": 5,
					"max_distance": 15,
					"hard_distance": 10,
					"on_leash": {
						"event": "minecraft:on_leash",
						"target": "self"
					},
					"on_unleash": {
						"event": "minecraft:on_unleash",
						"target": "self"
					}
				},
				"minecraft:on_death": {
					"event": "entity_event:death",
					"target": "self"
				}
			},
			// 发动攻击后
			"entity_module:angry_after": {
				"minecraft:timer": {
					"time": 1,
					"time_down_event": {
						"event": "entity_event:attack_execute",
						"target": "self"
					}
				},
				"minecraft:angry": {
					"broadcast_anger": true,
					"broadcast_range": 4,
					"duration": 32,
					"broadcast_targets": [
						"divine_creature"
					],
					"broadcast_filters": {
						"test": "is_visible",
						"subject": "other",
						"value": true
					},
					"filters": {
						"test": "is_visible",
						"subject": "target",
						"value": true
					},
					"calm_event": {
						"event": "entity_event:attack_done",
						"target": "self"
					}
				}
			}
		},
		"components": {
			// 基础属性
			"minecraft:type_family": {
				"family": [
					"divine_creature",
					"starry",
					"mob"
				]
			},
			"minecraft:collision_box": {
				"width": 1.0,
				"height": 0.6
			},
			"minecraft:health": {
				"value": 10
			},
			"minecraft:scale": {
				"value": 0.75
			},
			"minecraft:movement": {
				"value": 0.05
			},
			"minecraft:underwater_movement": {
				"value": 0.45
			},
			"minecraft:nameable": {
				"allow_name_tag_renaming": true,
				"always_show": true
			},
			"minecraft:breathable": {
				"generates_bubbles": true,
				"breathes_solids": true,
				"breathes_water": true,
				"breathes_lava": true,
				"breathes_air": true,
				"suffocate_time": 10,
				"total_supply": 15,
				"inhale_time": 0
			},
			"minecraft:is_stackable": {},
			"minecraft:fire_immune": true,
			// 物理属性
			"minecraft:pushable": {
				"is_pushable_by_piston": true,
				"is_pushable": true
			},
			"minecraft:physics": {
				"push_towards_closest_space": true,
				"has_collision": true,
				"has_gravity": true
			},
			// 事件触发器
			"minecraft:on_target_acquired": {
				"event": "entity_event:attack_point",
				"target": "self"
			},
			"minecraft:damage_sensor": {
				"triggers": [
					{
						"cause": "all",
						"damage_multiplier": 0.01
					},
					{
						"cause": "fall",
						"deals_damage": false
					}
				]
			},
			// 行为控制
			"minecraft:behavior.nearest_attackable_target": {
				"priority": 10,
				"entity_types": [
					{
						"filters": {
							"any_of": [
								{
									"test": "is_family",
									"subject": "other",
									"value": "abyss"
								},
								{
									"test": "is_family",
									"subject": "other",
									"value": "monster"
								},
								{
									"test": "is_family",
									"subject": "other",
									"value": "warden"
								},
								{
									"test": "is_family",
									"subject": "other",
									"value": "hoglin"
								}
							]
						},
						"within_default": 32
					}
				],
				"must_reach": true,
				"must_see": true
			},
			"minecraft:behavior.random_swim": {
				"avoid_surface": true,
				"priority": 30
			},
			"minecraft:behavior.swim_wander": {
				"priority": 25
			},
			"minecraft:behavior.swim_idle": {
				"priority": 35
			},
			"minecraft:navigation.generic": {
				"can_path_over_water": true,
				"is_amphibious": true,
				"can_breach": false,
				"can_walk": false,
				"can_sink": false,
				"can_swim": true
			},
			"minecraft:movement.generic": {}
		},
		"events": {
			// 实体诞生事件
			"minecraft:entity_spawned": {
				"add": {
					"component_groups": [
						"entity_module:controlled"
					]
				}
			},
			// 实体死亡事件
			"entity_event:death": {
				"queue_command": {
					"command": [
						"opal:entity_death_preservation @s"
					]
				}
			},
			// 实体契约事件
			"entity_event:create_contract_before": {
				"queue_command": {
					"command": [
						"opal:entity_contract_rite"
					]
				},
				"add": {
					"component_groups": [
						"entity_module:controlled"
					]
				}
			},
			// 实体攻击瞄准事件
			"entity_event:attack_point": {
				"add": {
					"component_groups": [
						"entity_module:angry_after"
					]
				}
			},
			// 实体攻击执行事件
			"entity_event:attack_execute": {
				"queue_command": {
					"command": [
						"opal:divine_favor_attack @s starry_family"
					]
				}
			},
			// 实体攻击结束事件
			"entity_event:attack_done": {
				"remove": {
					"component_groups": [
						"entity_module:angry_after"
					]
				}
			}
		}
	}
}