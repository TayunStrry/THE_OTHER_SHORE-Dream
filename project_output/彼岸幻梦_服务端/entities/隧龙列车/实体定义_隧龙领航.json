{
	"format_version": "1.21.10",
	"minecraft:entity": {
		"description": {
			"identifier": "starry_map:tunnel_dragon",
			"is_experimental": false,
			"is_summonable": true,
			"is_spawnable": false
		},
		"component_groups": {
			// 签订契约之后
			"entity_module:contract_after": {
				"minecraft:rideable": {
					"seat_count": 4,
					"family_types": [
						"train_parts"
					],
					"seats": [
						{
							"position": [
								1.305,
								0.57,
								-1.875
							],
							"min_rider_count": 1,
							"max_rider_count": 4,
							"rotate_rider_by": 90,
							"lock_rider_rotation": 90
						},
						{
							"position": [
								-1.305,
								0.57,
								-1.875
							],
							"min_rider_count": 2,
							"max_rider_count": 4,
							"rotate_rider_by": -90,
							"lock_rider_rotation": 90
						},
						{
							"position": [
								1.305,
								0.57,
								0.75
							],
							"min_rider_count": 3,
							"max_rider_count": 4,
							"rotate_rider_by": 90,
							"lock_rider_rotation": 90
						},
						{
							"position": [
								-1.305,
								0.57,
								0.75
							],
							"min_rider_count": 4,
							"max_rider_count": 4,
							"rotate_rider_by": -90,
							"lock_rider_rotation": 90
						}
					]
				},
				"minecraft:interact": {
					"interactions": [
						{
							"on_interact": {
								"filters": {
									"all_of": [
										{
											"test": "is_sneaking",
											"subject": "player",
											"operator": "=="
										},
										{
											"test": "is_owner",
											"subject": "other",
											"value": true
										}
									]
								},
								"event": "entity_event:recycling",
								"target": "self"
							},
							"interact_text": "回收 < 隧龙隧道列车 >",
							"play_sounds": "armor.equip_diamond",
							"use_item": false,
							"cooldown": 1
						},
						{
							"on_interact": {
								"filters": {
									"any_of": [
										{
											"test": "has_equipment",
											"domain": "hand",
											"subject": "other",
											"value": "starry_map:enlightenment"
										},
										{
											"test": "has_equipment",
											"domain": "hand",
											"subject": "other",
											"value": "starry_map:stateful_inspection"
										}
									]
								},
								"event": "entity_event:script_cope",
								"target": "self"
							},
							"interact_text": "",
							"play_sounds": "beacon.activate",
							"use_item": false,
							"cooldown": 1
						}
					]
				},
				"minecraft:on_death": {
					"event": "entity_event:death",
					"target": "self"
				},
				"minecraft:is_tamed": {}
			},
			// 实体套件 - 载具货仓
			"entity_module:inventory": {
				"minecraft:inventory": {
					"container_type": "inventory",
					"can_be_siphoned_from": true,
					"restrict_to_owner": true,
					"inventory_size": 27,
					"private": false
				},
				"minecraft:item_hopper": {}
			}
		},
		"components": {
			// 基础属性
			"minecraft:type_family": {
				"family": [
					"starry",
					"train"
				]
			},
			"minecraft:collision_box": {
				"width": 1,
				"height": 0.5
			},
			"minecraft:health": {
				"value": 500,
				"max": 500
			},
			"minecraft:scale": {
				"value": 1.0
			},
			"minecraft:knockback_resistance": {
				"value": 1.0
			},
			"minecraft:breathable": {
				"generates_bubbles": true,
				"breathes_solids": true,
				"breathes_water": true,
				"breathes_lava": true,
				"breathes_air": true,
				"suffocate_time": 10,
				"total_supply": 15,
				"inhale_time": 0
			},
			"minecraft:is_collidable": {},
			"minecraft:fire_immune": true,
			// 物理属性
			"minecraft:pushable": {
				"is_pushable_by_piston": false,
				"is_pushable": false
			},
			"minecraft:physics": {
				"push_towards_closest_space": true,
				"has_collision": true,
				"has_gravity": true
			},
			"minecraft:body_rotation_axis_aligned": {},
			// 事件触发器
			"minecraft:damage_sensor": {
				"triggers": [
					{
						"cause": "all",
						"on_damage": {
							"event": "entity_event:under_attack",
							"target": "self",
							"filters": {
								"any_of": [
									{
										"test": "is_family",
										"subject": "damager",
										"value": "player"
									},
									{
										"test": "is_family",
										"subject": "damager",
										"value": "starry"
									}
								]
							}
						},
						"deals_damage": false
					},
					{
						"cause": "fall",
						"deals_damage": false
					},
					{
						"cause": "projectile",
						"deals_damage": true
					}
				]
			},
			"minecraft:tameable": {
				"probability": 1,
				"tame_items": [
					"starry_map:faerie_contract",
					"starry_map:eternal_energy",
					"minecraft:pumpkin_pie"
				],
				"tame_event": {
					"event": "entity_event:create_contract_before",
					"target": "self"
				}
			},
			"minecraft:timer": {
				"time": 0.01,
				"time_down_event": {
					"event": "entity_event:function",
					"target": "self"
				}
			}
		},
		"events": {
			// 实体诞生事件
			"minecraft:entity_spawned": {
				"add": {
					"component_groups": [
						"entity_module:contract_before"
					]
				}
			},
			// 实体死亡事件
			"entity_event:death": {
				"queue_command": {
					"command": [
						"opal:entity_death_preservation @s"
					]
				}
			},
			// 实体契约事件
			"entity_event:create_contract_before": {
				"queue_command": {
					"command": [
						"give @p starry_map:mechanized_operation",
						"opal:entity_contract_rite"
					]
				},
				"add": {
					"component_groups": [
						"entity_module:contract_after"
					]
				},
				"remove": {
					"component_groups": [
						"entity_module:contract_before"
					]
				}
			},
			// 实体事件 - 载具回收
			"entity_event:recycling": {
				"queue_command": {
					"command": [
						"particle constant:fireworks_fireball_rune_purple ~~~",
						"particle constant:fireworks_paper_rune_purple ~~~",
						"opal:tunnel_dragon_travel @s recycling"
					]
				}
			},
			// 实体事件 - 载具开关
			"entity_event:switch": {
				"sequence": [
					// 关闭 载具运行
					{
						"filters": {
							"test": "has_tag",
							"operator": "==",
							"value": "tunnel_train.function_open"
						},
						"queue_command": {
							"command": [
								"tag @s remove tunnel_train.function_open"
							]
						}
					},
					// 开启 载具运行
					{
						"filters": {
							"test": "has_tag",
							"operator": "!=",
							"value": "tunnel_train.function_open"
						},
						"queue_command": {
							"command": [
								"tag @s add tunnel_train.function_open"
							]
						},
						"add": {
							"component_groups": [
								"entity_module:inventory"
							]
						},
						"remove": {
							"component_groups": [
								"entity_module:economy"
							]
						}
					}
				]
			},
			// 实体事件 - 载具功能
			"entity_event:function": {
				"sequence": [
					// 执行 常规行驶 的任务
					{
						"filters": {
							"all_of": [
								{
									"test": "has_tag",
									"operator": "==",
									"value": "tunnel_train.function_open"
								}
							]
						},
						"sequence": [
							// 执行 挖掘方块 照亮环境 消耗能量 铺设方块 的任务
							{
								"filters": {
									"all_of": [
										{
											"test": "has_tag",
											"operator": "!=",
											"value": "tunnel_train.Dextral"
										},
										{
											"test": "has_tag",
											"operator": "!=",
											"value": "tunnel_train.Sinistral"
										}
									]
								},
								"sequence": [
									// 列车行驶
									{
										"queue_command": {
											"command": [
												"opal:tunnel_dragon_travel @s train_travel"
											]
										}
									},
									// 仓储过滤
									{
										"queue_command": {
											"command": [
												"opal:tunnel_dragon_travel @s inventory_filter"
											]
										}
									},
									//收集附近的掉落物
									{
										"queue_command": {
											"command": [
												"tp @e[type=item,r=5] @s"
											]
										}
									}
								]
							}
						]
					},
					// 执行 车辆向右旋转 的任务
					{
						"filters": {
							"all_of": [
								{
									"test": "has_tag",
									"operator": "==",
									"value": "tunnel_train.Dextral"
								},
								{
									"test": "has_tag",
									"operator": "!=",
									"value": "tunnel_train.Sinistral"
								}
							]
						},
						"queue_command": {
							"command": [
								"tag @s remove tunnel_train.Dextral",
								"tp @s ~~~ ~90"
							]
						}
					},
					// 执行 车辆向左旋转 的任务
					{
						"filters": {
							"all_of": [
								{
									"test": "has_tag",
									"operator": "!=",
									"value": "tunnel_train.Dextral"
								},
								{
									"test": "has_tag",
									"operator": "==",
									"value": "tunnel_train.Sinistral"
								}
							]
						},
						"queue_command": {
							"command": [
								"tag @s remove tunnel_train.Sinistral",
								"tp @s ~~~ ~-90"
							]
						}
					}
				]
			},
			// 实体事件 - 载具行驶
			"entity_event:travel": {
				"sequence": [
					//用于使车辆前进 并消耗能量 同时赋予玩家状态效果
					{
						"queue_command": {
							"command": [
								"tp @s ^ ^ ^0.2",
								"effect @a[r=15] conduit_power 11 0 true",
								"effect @a[r=15] fire_resistance 11 0 true"
							]
						}
					},
					//用于处理车 辆周围 的光照方块 并 挖掘 前方的方块
					{
						"queue_command": {
							"command": [
								"fill ^2 ^ ^3 ^-2 ^4 ^-3 air [] destroy"
							]
						}
					},
					//用于铺设 垫脚 的平台
					{
						"queue_command": {
							"command": [
								"fill ^1 ^-1 ^ ^-1 ^-1 ^2 minecraft:cobblestone_slab [\"stone_slab_type\"=\"cobblestone\", \"top_slot_bit\"=true, \"minecraft:vertical_half\"=\"top\"] replace minecraft:water",
								"fill ^1 ^-1 ^ ^-1 ^-1 ^2 minecraft:cobblestone_slab [\"stone_slab_type\"=\"cobblestone\", \"top_slot_bit\"=true, \"minecraft:vertical_half\"=\"top\"] keep"
							]
						}
					}
				]
			},
			// 实体事件 - 载具被攻击
			"entity_event:under_attack": {
				"queue_command": {
					"command": [
						"opal:tunnel_dragon_travel @s extend_module"
					]
				}
			}
		}
	}
}