{
	"format_version": "1.21.10",
	"minecraft:entity": {
		"description": {
			"identifier": "starry_map:execute.free_pointer",
			"is_experimental": false,
			"is_summonable": true,
			"is_spawnable": false,
			"properties": {
				"property:x_axle_rotate": {
					"type": "float",
					"range": [
						-90.0,
						90.0
					],
					"default": 0.0,
					"client_sync": true
				},
				"property:y_axle_rotate": {
					"type": "float",
					"range": [
						-180.0,
						180.0
					],
					"default": 0.0,
					"client_sync": true
				},
				"property:length": {
					"type": "float",
					"range": [
						1.0,
						9999.0
					],
					"default": 1.0,
					"client_sync": true
				},
				"property:color_r": {
					"type": "float",
					"range": [
						0.0,
						1.0
					],
					"default": "math.random(0.0, 1.0)",
					"client_sync": true
				},
				"property:color_g": {
					"type": "float",
					"range": [
						0.0,
						1.0
					],
					"default": "math.random(0.0, 1.0)",
					"client_sync": true
				},
				"property:color_b": {
					"type": "float",
					"range": [
						0.0,
						1.0
					],
					"default": "math.random(0.0, 1.0)",
					"client_sync": true
				},
				"property:color_a": {
					"type": "float",
					"range": [
						0.0,
						1.0
					],
					"default": "math.random(0.0, 1.0)",
					"client_sync": true
				}
			}
		},
		"component_groups": {
			// 实体销毁
			"entity_module:despawn": {
				"minecraft:instant_despawn": {
					"remove_child_entities": true
				}
			}
		},
		"components": {
			// 基础属性
			"minecraft:collision_box": {
				"height": 0.0,
				"width": 0.0
			},
			"minecraft:health": {
				"value": 1,
				"min": 1
			},
			"minecraft:scale": {
				"value": 1
			},
			"minecraft:knockback_resistance": {
				"value": 1
			},
			"minecraft:nameable": {
				"always_show": true
			},
			"minecraft:fire_immune": true,
			// 物理属性
			"minecraft:pushable": {
				"is_pushable_by_piston": false,
				"is_pushable": false
			},
			"minecraft:physics": {
				"push_towards_closest_space": false,
				"has_collision": false,
				"has_gravity": false
			},
			//事件触发器
			"minecraft:damage_sensor": {
				"triggers": [
					{
						"cause": "all",
						"deals_damage": false
					}
				]
			},
			"minecraft:timer": {
				"time": 15,
				"time_down_event": {
					"event": "entity_event:delete",
					"target": "self"
				}
			},
			// 行为控制
			"minecraft:conditional_bandwidth_optimization": {
				"conditional_values": {
					"max_dropped_ticks": 20,
					"max_optimized_distance": 200,
					"use_motion_prediction_hints": true
				},
				"default_values": {
					"max_dropped_ticks": 10,
					"max_optimized_distance": 100,
					"use_motion_prediction_hints": true
				}
			},
			"minecraft:navigation.hover": {
				"can_path_from_air": true,
				"is_amphibious": true,
				"can_swim": true,
				"can_sink": true
			},
			"minecraft:movement.hover": {}
		},
		"events": {
			// 实体销毁事件
			"entity_event:delete": {
				"add": {
					"component_groups": [
						"entity_module:despawn"
					]
				}
			}
		}
	}
}