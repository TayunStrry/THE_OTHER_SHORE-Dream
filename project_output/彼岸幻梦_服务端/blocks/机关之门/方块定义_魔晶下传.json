{
	"format_version": "1.21.90",
	"minecraft:block": {
		"description": {
			"menu_category": {
				"category": "items"
			},
			"identifier": "starry_map:magic_portal_below",
			"states": {
				"STATE:rune_type": [
					0,
					1,
					2,
					3,
					4,
					5,
					6,
					7,
					9
				],
				"STATE:direction_0": [
					0,
					1
				],
				"STATE:direction_1": [
					0,
					1
				],
				"STATE:direction_2": [
					0,
					1
				],
				"STATE:direction_3": [
					0,
					1
				]
			}
		},
		"components": {
			// 方块含水
			"minecraft:liquid_detection": {
				"detection_rules": [
					{
						"on_liquid_touches": "no_reaction",
						"can_contain_liquid": true,
						"liquid_type": "water"
					}
				]
			},
			// 防爆与挖掘
			"minecraft:destructible_by_explosion": false,
			"minecraft:destructible_by_mining": {
				"seconds_to_destroy": 1
			},
			// 摩擦力
			"minecraft:friction": 0.35,
			// 方块光照
			"minecraft:light_dampening": 0,
			"minecraft:light_emission": 1,
			// 模型与纹理
			"minecraft:material_instances": {
				"*": {
					"texture": "block_texture:transmit.green",
					"render_method": "alpha_test"
				},
				"stone_1": {
					"texture": "polished_diorite_slab",
					"render_method": "alpha_test"
				},
				"stone_7": {
					"texture": "polished_blackstone",
					"render_method": "alpha_test"
				}
			},
			"minecraft:geometry": {
				"identifier": "geometry.block.gate.teleport",
				"bone_visibility": {
					"frame_0": "q.block_state('STATE:direction_0') == 0",
					"frame_1": "q.block_state('STATE:direction_1') == 0",
					"frame_2": "q.block_state('STATE:direction_2') == 0",
					"frame_3": "q.block_state('STATE:direction_3') == 0",
					"plane_0": "q.block_state('STATE:direction_0') == 1",
					"plane_1": "q.block_state('STATE:direction_1') == 1",
					"plane_2": "q.block_state('STATE:direction_2') == 1",
					"plane_3": "q.block_state('STATE:direction_3') == 1"
				}
			},
			// 碰撞盒与选择盒
			"minecraft:collision_box": {
				"origin": [
					-8,
					2,
					-8
				],
				"size": [
					16,
					2,
					16
				]
			},
			"minecraft:selection_box": {
				"origin": [
					-8,
					0,
					-8
				],
				"size": [
					16,
					6,
					16
				]
			},
			// 自定义组件
			"opal:interact.magic_crystal_download": {},
			// 方块标签
			"tag:tags:magic_cable.series": {},
			"tag:tags:machine_gate.series": {},
			"tag:tags:magic_portal.series": {},
			"tag:tags:magic_cable.port_negative.X": {},
			"tag:tags:magic_cable.port_positive.X": {},
			"tag:tags:magic_cable.port_positive.Z": {},
			"tag:tags:magic_cable.port_negative.Z": {}
		},
		"permutations": [
			{
				"condition": "q.block_state('STATE:rune_type') != 0 && q.block_state('STATE:rune_type') != 9",
				"components": {
					"opal:tick.magic_crystal_download": {},
					"minecraft:tick": {
						"interval_range": [
							2,
							2
						]
					}
				}
			},
			{
				"condition": "q.block_state('STATE:rune_type') == 9",
				"components": {
					"opal:tick.magic_crystal_download": {},
					"minecraft:tick": {
						"interval_range": [
							20,
							20
						]
					}
				}
			}
		]
	}
}